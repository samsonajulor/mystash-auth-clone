{
  "name": "mystash-auth",
  "version": "0.0.0",
  "description": "Auth + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "push": "bash ./modules/push.sh",
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "test:single": "yarn node --experimental-vm-modules $(yarn bin jest) $1",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn lint -- --fix",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate",
    "deploy:prod": "yarn build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "setup": "yarn && yarn init:mod $1",
    "init:mod": "bash ./scripts/modules.sh",
    "init:requests": "bash ./scripts/requests.sh"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "@redis/client": "^1.6.0",
    "@sendgrid/mail": "^8.1.3",
    "amqplib": "^0.10.4",
    "axios": "^1.7.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^2.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.0.1",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "js2xmlparser": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "ksuid": "^3.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.1",
    "mongoose-lean-virtuals": "^0.9.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "otplib": "^12.0.1",
    "plaid": "^26.0.0",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.6",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "^22.1.0",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "prettier": "^2.7.1",
    "prisma": "^4.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.5.4"
  }
}
